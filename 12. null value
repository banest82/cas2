using System;


namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            int? b1 = null;
            double? b2 = null;
            Console.WriteLine("b1 is {0} and b2 is {1}", b1, b2);
            b1 = 100;
            b2 = 1.8596;
            Console.WriteLine("b1 is {0} and b2 is {1}", b1, b2);



            Console.ReadLine();
        }
    }
}
