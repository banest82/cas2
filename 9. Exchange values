using System;


namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            int numb1 = 5;
            int numb2 = 10;
            Console.WriteLine("numb1 is {0} and numb2 is {1}", numb1, numb2);
            int numb3 = numb1 + numb2;
            numb2 = numb3 - numb2;
            numb1 = numb3 - numb2;
            Console.WriteLine("numb1 is {0} and numb2 is {1}", numb1, numb2);
            Console.ReadLine();
        }
    }
}
