using System;


namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            string FirstName = "Bane";
            string LastName = "Stajic";
            byte Age = 34;
            char Gender = 'm';
            long IdNumber = 3009982722231;
            int UidNumber = 250314;

            Console.WriteLine("First name: {0}\nLast Name: {1}\nAge: {2}\nGender: {3}\nId Number: {4}\nUser ID: {5}"
                , FirstName, LastName, Age, Gender, IdNumber, UidNumber );
            Console.ReadLine();
        }
    }
}
